<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension-point id="navbarItem" name="Navbar Item" schema="schema/navbarItem.exsd"/>
   <extension-point id="config" name="Configuration Extensions" schema="schema/config.exsd"/>
   <extension-point id="configTab" name="Configuration Tabs" schema="schema/configTab.exsd"/>
   <extension
         point="org.eclipse.equinox.http.registry.httpcontexts">
      <httpcontext
            id="jabylon">
      </httpcontext>
   </extension>
    <extension
         point="org.eclipse.equinox.http.registry.filters">
      <filter
            alias="/"
            class="org.jabylon.rest.ui.JabylonFilter"
            httpcontextId="jabylon">
         <init-param
               name="applicationClassName"
               value="org.jabylon.rest.ui.wicket.JabylonApplication">
         </init-param>
         <init-param
               name="filterMappingUrlPattern"
               value="/*">
         </init-param>
         <init-param
               name="configuration"
               value="deployment">
         </init-param>
      </filter>
   </extension>
 <extension
       point="org.jabylon.rest.ui.navbarItem">
    <item
          panel="org.jabylon.rest.ui.navbar.SettingsNavBarPanel$SettingsPanelFactory"
          pullRight="true">
    </item>
    <item
          panel="org.jabylon.rest.ui.navbar.LoginPanel$LoginPanelFactory"
          pullRight="true">
    </item>
    <item
          panel="org.jabylon.rest.ui.navbar.HelpPanel$HelpPanelFactory">
    </item>
    <item
          panel="org.jabylon.rest.ui.navbar.SearchPanel$SearchPanelFactory">
    </item>
 </extension>
 <extension
       point="org.jabylon.rest.ui.configTab">
    <tab
          name="General"
          precedence="10"
          tabID="general">
    </tab>
    <tab
          name="Scanning"
          precedence="20"
          tabID="scanning">
    </tab>
    <tab
          name="Checks"
          precedence="30"
          tabID="checks">
    </tab>
    <tab
          name="Team"
          precedence="40"
          tabID="team">
    </tab>
    <tab
          name="Versions"
          precedence="50"
          tabID="versions">
    </tab>
    <tab
          name="Users"
          precedence="60"
          tabID="users">
    </tab>
    <tab
          name="Groups"
          precedence="70"
          tabID="groups">
    </tab>
    <tab
          name="Roles"
          precedence="80"
          tabID="roles">
    </tab>
    <tab
          name="Permissions"
          precedence="90"
          tabID="permissions">
    </tab>
    <tab
          name="Index"
          precedence="100"
          tabID="index">
    </tab>
 </extension>
 <extension
       point="org.jabylon.rest.ui.config">
    <section
          name="General Workspace Settings"
          objectClass="org.jabylon.properties.Workspace"
          section="org.jabylon.rest.ui.wicket.config.sections.GeneralWorkspaceConfig"
          tab="general">
    </section>
    <section
          name="General Project Settings"
          objectClass="org.jabylon.properties.Project"
          section="org.jabylon.rest.ui.wicket.config.sections.GeneralProjectConfig"
          tab="general">
    </section>
    <section
          name="Scanning"
          objectClass="org.jabylon.properties.Project"
          section="org.jabylon.rest.ui.wicket.config.sections.ScanningConfigSection$ScanningConfig"
          tab="scanning">
    </section>
    <section
          name="Versions"
          objectClass="org.jabylon.properties.Project"
          section="org.jabylon.rest.ui.wicket.config.sections.ProjectVersionsConfigSection$VersionsConfig"
          tab="versions">
    </section>
    <section
          name="Version Settings"
          objectClass="org.jabylon.properties.ProjectVersion"
          section="org.jabylon.rest.ui.wicket.config.sections.VersionConfigSection$VersionConfig"
          tab="general">
    </section>
    <section
          name="Translation Checks"
          objectClass="org.jabylon.properties.Project"
          section="org.jabylon.rest.ui.wicket.config.sections.TranslationChecksConfigSection$TranslationChecksConfig"
          tab="checks">
    </section>
    <section
          name="User Management"
          objectClass="org.jabylon.users.UserManagement"
          section="org.jabylon.rest.ui.wicket.config.sections.security.UsersConfigSection$UsersConfigSectionContributor"
          tab="users">
    </section>
    <section
          name="User"
          objectClass="org.jabylon.users.User"
          section="org.jabylon.rest.ui.wicket.config.sections.security.UserConfigSection$UserConfigSectionContributor"
          tab="general">
    </section>
    <section
          name="User Roles"
          objectClass="org.jabylon.users.User"
          section="org.jabylon.rest.ui.wicket.config.sections.security.UserRolesConfigSection$UserRolesConfigSectionContributor"
          tab="roles">
    </section>
    <section
          name="Role Management"
          objectClass="org.jabylon.users.UserManagement"
          section="org.jabylon.rest.ui.wicket.config.sections.security.RolesConfigSection$RolesConfigSectionContributor"
          tab="roles">
    </section>
    <section
          name="Role Permissions"
          objectClass="org.jabylon.users.Role"
          section="org.jabylon.rest.ui.wicket.config.sections.security.RolePermissionsConfigSection$RolePermissionsConfigSectionContributor"
          tab="permissions">
    </section>
    <section
          name="Permissions"
          objectClass="org.jabylon.properties.Project"
          section="org.jabylon.rest.ui.wicket.config.sections.security.ProjectPermissionsConfigSection$ProjectPermissionsConfigSectionContributor"
          tab="permissions">
    </section>
    <section
          name="Index Settings"
          objectClass="org.jabylon.properties.Workspace"
          section="org.jabylon.rest.ui.wicket.config.sections.IndexingConfigSection$IndexingConfig"
          tab="index">
    </section>
 </extension>
 <extension
       point="org.eclipse.equinox.http.registry.resources">
    <resource
          alias="/"
          base-name="WebContent">
    </resource>
 </extension>
</plugin>
